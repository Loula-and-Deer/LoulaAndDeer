<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<link rel="canonical" href="{{ canonical_url }}" />

<!-- Verification (Google and Microsoft (Bing) -->
<meta name="google-site-verification" content="BDt6zg4W_YgDvPXbxNRyTZ9CqADLQoZvqULXSu-Vx_8" />
<meta name="msvalidate.01" content="C8BDDE326B27793994EACFDF0A558030" />

<!-- Icon meta (Android, iOS and Windows -->
<meta name="apple-mobile-web-app-title" content="Loula and Deer">
<meta name="application-name" content="Loula and Deer">
<meta name="msapplication-TileColor" content="#5d5d5d">
<meta name="msapplication-TileImage" content="{{ 'mstile-144x144.png' | asset_url }}">
<meta name="msapplication-config" content="{{ 'browserconfig.xml' | asset_url }}">
<meta name="theme-color" content="#5d5d5d">
  
<!-- Icon links -->
<link rel="apple-touch-icon" sizes="180x180" href="{{ 'apple-touch-icon.png' | asset_url }}">
<link rel="icon" type="image/png" sizes="32x32" href="{{ 'favicon-32x32.png' | asset_url }}">
<link rel="icon" type="image/png" sizes="16x16" href="{{ 'favicon-16x16.png' | asset_url }}">
<link rel="manifest" href="{{ 'manifest.json' | asset_url }}">
<link rel="mask-icon" color="#5d5d5d" href="{{ 'safari-pinned-tab.svg' | asset_url }}" >
<link rel="shortcut icon" href="{{ 'favicon.ico' | asset_url }}">

<!-- Other meta information -->
<title>{{ page_title }}
  {% if template != 'index' %} - {{ shop.name }} {% else %} - Baby gifts and toys {% endif %}
  {% if template == 'search' %}Search{% endif %}</title>

{% if page_description %}
<meta name="description" content="{{ page_description | escape }}" />
{% endif %}

<meta name="keywords" content="
baby, baby toys, children's toys, children's clothing, children's accessories, children,
personalised gifts, personalised,
ballerina, ballerina music box, music box,
soft toys, wooden toys, puppets, hand puppets, mouse toys, spinning top, toys, train,
brighton, the lanes,
maileg, petit bateau, I love gorgeous, Wild and gorgeous" />

<!-- Vendor meta information -->
{% include 'fb-open-graph-tags' %}
{% include 'twitter-cards' %}

<!-- External styles -->
{{ 'styles.scss.css' | asset_url | stylesheet_tag }}
{{ 'social/social-icons.css' | global_asset_url | stylesheet_tag }}
{{ 'font-awesome.css' | asset_url | stylesheet_tag }}
{{ 'shopstorm-apps.scss.css' | asset_url | stylesheet_tag }}

<!-- Google fonts -->
{% if settings.google_header_font != "none" %}
<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.google_header_font }}:300,400,700">
{% endif %}
{% if settings.google_nav_font != "none" %}
<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.google_nav_font }}:300,400,700">
{% endif %}


<!-- External scripts -->
<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.7/jquery.min.js"></script>

{% if template contains 'customers' %}
<script src="{{ 'shopify_common.js' | shopify_asset_url}}"></script>
<script src="{{ 'customer_area.js' | shopify_asset_url}}"></script>
{% endif %}

<script src="{{ 'option_selection.js' | shopify_asset_url}}"></script>
<script src="{{ 'api.jquery.js' | shopify_asset_url}}"></script>

